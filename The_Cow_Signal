#include <iostream>
using namespace std;

int main() {
    freopen("cowsignal.in", "r", stdin);
    freopen("cowsignal.out", "w", stdout);
    int M, N, K;
    cin >> M >> N >> K;
    for (int i = 0; i < M; i++) {
        string str;
        cin >> str;
        for (int j = 0; j < K; j++) {
            for (int k = 0; k < N; k++) {
                for (int l = 0; l < K; l++) {
                    cout << str[k];
                }
            }
            cout << endl;
        }
    }
    return 0;
}
