#include <iostream>
using namespace std;

int main() {
	int capacity[3], milk[3];
	for (int i = 0; i < 3; i++)
		cin >> capacity[i] >> milk[i];
	for (int i = 0; i < 100; i++){
		int maximum = capacity[(i + 1) % 3] - milk[(i + 1) % 3];
		if (milk[i % 3] <= maximum) {
			milk[(i + 1) % 3] += milk[i % 3];
			milk[i % 3] = 0;
		}
		else {
			milk[(i + 1) % 3] = capacity[(i + 1) % 3];
			milk[i % 3] -= maximum;
		}
	}
	for (int i = 0; i < 3; i++)
		cout << milk[i] << endl;
	return 0;
}
